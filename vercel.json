{
  "version": 2,
  "name": "lighthouse-api",
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ],
  "env": {
    "PRISMA_CLI_BINARY_TARGETS": "rhel-openssl-1.0.x"
  }
}
